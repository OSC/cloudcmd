<!-- OSC_CUSTOM_CODE change title="clear storage (Ctrl+D)" to title="clear clipboard (Ctrl+D)" -->
<div data-name="js-path" class="reduce-text" title="{{ fullPath }}"><span data-name="js-clear-storage" class="path-icon icon-clear" title="clear clipboard (Ctrl+D)"></span><a data-name="js-refresh" href="{{ link }}" class="path-icon icon-refresh" title="refresh (Ctrl+R)"></a><span data-name="js-links" class=links>{{ path }}</span></div>
